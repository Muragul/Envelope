<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!--    On Boarding-->
    <style name="Button.Text" parent="Widget.MaterialComponents.Button.TextButton">
        <item name="android:layout_width">wrap_content</item>
        <item name="textStartPadding">0dp</item>
        <item name="textEndPadding">0dp</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:letterSpacing">0.0</item>
        <item name="cornerRadius">10dp</item>
        <item name="android:textColor">@color/black</item>
    </style>

    <!--    Start Page-->
    <style name="ShapeAppearanceOverlay.App.CornerSize50Percent" parent="">
        <item name="cornerSize">50%</item>
    </style>

    <style name="mButtonStyle" parent="Widget.MaterialComponents.Button">
        <item name="backgroundTint">@color/button_selector</item>
    </style>

</resources>